  <DataSets>
    <DataSet Name="Products_DataSet">
      <Query>
        <DataSourceName>Adv_DataSource</DataSourceName>
        <CommandText>/****** Script for SelectTopNRows command from SSMS  ******/
SELECT TOP 1000 [ProductID]
      ,a.[Name]
      ,[ProductNumber]
      ,[MakeFlag]
      ,[FinishedGoodsFlag]
      ,[Color]
      ,[SafetyStockLevel]
      ,[ReorderPoint]
      ,[StandardCost]
      ,[ListPrice]
      ,[Size]
      ,[SizeUnitMeasureCode]
      ,[WeightUnitMeasureCode]
      ,[Weight]
      ,[DaysToManufacture]
      ,[ProductLine]
      ,[Class]
      ,[Style]
      ,[ProductModelID]
      ,[SellStartDate]
      ,[SellEndDate]
      ,[DiscontinuedDate]
	  ,b.Name SubCategory
  FROM [AdventureWorks2014].[Production].[Product] a
  inner join [AdventureWorks2014].[Production].[ProductSubcategory] b
  on a.ProductSubCategoryId = b.ProductSubCategoryId
  where Color is not null</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
